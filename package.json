{
  "name": "rosmaro-tools",
  "version": "0.0.3",
  "author": "Åukasz Makuch @lukaszmakuch",
  "bin": {
    "rosmaro-tools": "./bin/run"
  },
  "bugs": "https://github.com/lukaszmakuch/rosmaro-tools/issues",
  "dependencies": {
    "@oclif/command": "^1.4.36",
    "@oclif/config": "^1.6.33",
    "@oclif/plugin-help": "^2.0.5",
    "escodegen": "^1.11.0",
    "glob": "^7.1.2",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/escodegen": "0.0.6",
    "@types/glob": "^5.0.35",
    "@oclif/dev-cli": "^1.15.4",
    "@oclif/test": "^1.1.0",
    "@oclif/tslint": "^1.1.2",
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.4",
    "chai": "^4.1.2",
    "globby": "^8.0.1",
    "mocha": "^5.0.5",
    "nyc": "^12.0.2",
    "ts-node": "^6.2.0",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/lukaszmakuch/rosmaro-tools",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "rosmaro-tools",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "bindings": {
        "description": "manage bindings"
      }
    }
  },
  "repository": "lukaszmakuch/rosmaro-tools",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
    "posttest": "#tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "prepare": "rm -rf lib && tsc",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
